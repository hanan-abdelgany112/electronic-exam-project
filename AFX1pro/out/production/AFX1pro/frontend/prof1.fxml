<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #22577E;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controaller.profController">
   <children>
      <Label layoutX="107.0" layoutY="60.0" text="Welcome (name of prof)" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button layoutX="107.0" layoutY="149.0" mnemonicParsing="false" onAction="#onClickAddExam" style="-fx-background-color: #ffffff, #22577E;" text="add exam" textFill="#f8f5f5">
         <font>
            <Font name="Arial Narrow Bold" size="24.0" />
         </font>
      </Button>
      <Button layoutX="319.0" layoutY="155.0" mnemonicParsing="false" onAction="#OnClickDleteExam" style="-fx-background-color: #ffffff, #22577E;" text="delete exam" textFill="#f8f5f5">
         <font>
            <Font name="Arial Narrow Bold" size="24.0" />
         </font>
      </Button>
      <Label layoutX="179.0" layoutY="344.0" prefHeight="17.0" prefWidth="86.0" />
      <ImageView fitHeight="68.0" fitWidth="80.0" layoutX="515.0" layoutY="323.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/java.png" />
         </image>
      </ImageView>
      <Button layoutX="242.0" layoutY="322.0" mnemonicParsing="false" onAction="#onclickExit" prefHeight="25.0" prefWidth="95.0" text="Exit">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="212.0" layoutY="237.0" mnemonicParsing="false" onAction="#onClickShowGrades" style="-fx-background-color: #ffffff, #22577E;" text="Show grades" textFill="#f8f5f5">
         <font>
            <Font name="Arial Narrow Bold" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
